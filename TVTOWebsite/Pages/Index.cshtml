    <script type="text/javascript">
        function Slider() {
            var me100 = this;
            me100.contador = 100;
            me100.maxNoticias = 103;
            me100.delay = 4000;
            me100.timer = 0;
            this.iniciar = function () {
                me100.timer = setInterval(this.transicao, me100.delay);
            };
            this.transicao = function (Xrotate) {
                if (((Number(Xrotate) > Number(100)) && (Number(Xrotate) <= me100.maxNoticias)) && (Number(Xrotate) != me100.contador)) {
                    if ($('#estado_' + me100.contador).val() == "visivel") {
                        $('#estado_' + me100.contador).val('invisivel');
                        $('#bullet_' + me100.contador).attr({
                            src: "/CmsModules/News/NewsJquery/Jquery/image/bullet.png"
                        });
                        $('#div_' + me100.contador).fadeOut('slow', function () {
                            me100.contador = Number(Xrotate);
                            if ($('#estado_' + me100.contador).val() == "invisivel") {
                                $('#estado_' + me100.contador).val('visivel');
                                $('#bullet_' + me100.contador).attr({
                                    src: "/CmsModules/News/NewsJquery/Jquery/image/bullet_on.png"
                                });
                                $('#div_' + me100.contador).fadeIn('slow', function () {
                                    clearInterval(me100.timer);
                                    me100.timer = null;
                                    me100.delay = 12000;
                                    me100.iniciar();
                                });
                            }
                        });
                    }
                } else {
                    if ($('#estado_' + me100.contador).val() == "visivel") {
                        $('#estado_' + me100.contador).val('invisivel');
                        $('#bullet_' + me100.contador).attr({
                            src: "/CmsModules/News/NewsJquery/Jquery/image/bullet.png"
                        });
                        $('#div_' + me100.contador).fadeOut('slow', function () {
                            me100.contador = me100.contador + 1;
                            if (me100.contador > 103) {
                                me100.contador = 100;
                            }
                            Xrotate = this.transicao;
                            if ($('#estado_' + me100.contador).val() == "invisivel") {
                                $('#estado_' + me100.contador).val('visivel');
                                $('#bullet_' + me100.contador).attr({
                                    src: "/CmsModules/News/NewsJquery/Jquery/image/bullet_on.png"
                                });
                                $('#div_' + me100.contador).fadeIn('slow', function () {
                                    clearInterval(me100.timer);
                                    me100.timer = null;
                                    me100.iniciar();
                                });
                            }
                        });
                    }
                }
            }
        }
        var obj100 = new Slider();
        obj100.iniciar();
    </script>

