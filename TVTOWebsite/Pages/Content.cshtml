    <script type="text/javascript">

        function CommentForm(id) {
            $("#divComment".concat(id)).html($(".divComment").html());
            $(".divComment").html("");
            $(".btnSubmitAnswer").css("display", "block");
            $(".btnCancelAnswer").css("display", "block");
            $(".btnSubmit").css("display", "none");
            $("#ctl03_ctl01_CommentID").val(id);

            $("#refreshsecretcode").click(function () {
                $("#ctl03_ctl01_imgsecurity").attr("src", "/Security_Code.ashx?page=SubjectViewComment&w=105&h=28&rnd=" + Math.random().toString());
            });

            $('#ctl03_ctl01_btnCancelAnswer').click(function (e) {
                e.preventDefault();
                window.location.reload();
            });

            $('#ctl03_ctl01_btnSubmitAnswer').click(function (e) {
                e.preventDefault();

                var captcha = $('#ctl03_ctl01_txtamniyati');
                var name = $('#ctl03_ctl01_txtName');
                var family = $('#ctl03_ctl01_txtLastName');
                var email = $('#ctl03_ctl01_txtEmail');
                var comment = $('#ctl03_ctl01_txtNazar');
                var moduleType = $('#ctl03_ctl01_ModuleType');
                var id = $('#ctl03_ctl01_pId');
                var commentId = $('#ctl03_ctl01_CommentID');

                if (captcha.val() != '' && comment.val() != '') {

                    $('#ctl03_ctl01_btnSubmitAnswer').hide();
                    $('#ctl03_ctl01_btnCancelAnswer').hide();
                    $('#loadingbox').show();

                    var jsonData = {
                        'method': 'AnswerComment', 'name': name.val(), 'family': family.val(), 'email': email.val(),
                        'comment': comment.val(), 'moduleType': moduleType.val(), 'captcha': captcha.val(), 'id': id.val(), 'commentId': commentId.val()
                    };

                    $.post('/Ajax/Subject/Subject.ashx', jsonData, function (response) {
                        if (response.result == "True") {
                            $('#loadingbox').hide();
                            $('#ctl03_ctl01_btnSubmitAnswer').remove();
                            $('#ctl03_ctl01_btnCancelAnswer').remove();
                            $('#ctl03_ctl01_lblComentmsg').text(response.message);
                            name.val('');
                            family.val('');
                            email.val('');
                            comment.val('');
                            captcha.val('');
                            changeimg();
                        }
                        else {

                            $('#ctl03_ctl01_btnSubmitAnswer').show();
                            $('#ctl03_ctl01_btnCancelAnswer').show();
                            $('#loadingbox').hide();
                            $('#ctl03_ctl01_lblmsg').text(response.message);
                            document.getElementById("topbar").style.visibility = "visible";
                        }
                    });
                }
            });
        }

    </script>
